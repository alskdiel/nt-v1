<!DOCTYPE html>
<html>
  <head>
    <title>NtV1</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
    <script src='http://ajax.microsoft.com/ajax/jquery.templates/beta1/jquery.tmpl.min.js'></script>
    <script type="text/javascript" src="//apis.daum.net/maps/maps3.js?apikey=69efb28067502ad96827758bec645be3&libraries=services"></script>
  </head>
  <body ng-app="myApp" ng-controller="MainCtrl">
    <div class="main-container">
      <%= render template: "layouts/navbar" %>
      <div class="content-container">
        <%= yield %>
      </div>
    </div>
    <%= render template: "layouts/sidebar" %>
    <%= render template: "modals/templates" %>
  </body>
</html>
